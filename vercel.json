{
  "version": 2,
  "buildCommand": "npm install && npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/socket.io/(.*)",
      "dest": "https://your-backend-url.com/socket.io/$1"
    },
    {
      "src": "/api/(.*)",
      "dest": "https://your-backend-url.com/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "VITE_ADMIN_PASSWORD": "tee_shine18",
    "VITE_EMAIL_SERVICE_ID": "service_mzam0ga",
    "VITE_EMAIL_TEMPLATE_ID": "template_5uv23ip",
    "VITE_EMAIL_PUBLIC_KEY": "9sf1untPPvbg0U1P9",
    "VITE_WELCOME_TEMPLATE_ID": "template_qkfkuzi",
    "VITE_UPDATE_NOTIFICATION_TEMPLATE_ID": "template_5uv23ip",
    "VITE_BACKEND_URL": "https://your-backend-url.com"
  }
} 