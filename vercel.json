{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/assets/(.*)",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      },
      "continue": true
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/javascript"
        }
      ]
    }
  ],
  "env": {
    "VITE_ADMIN_PASSWORD": "tee_shine18",
    "VITE_EMAIL_SERVICE_ID": "service_mzam0ga",
    "VITE_EMAIL_TEMPLATE_ID": "template_5uv23ip",
    "VITE_EMAIL_PUBLIC_KEY": "9sf1untPPvbg0U1P9",
    "VITE_WELCOME_TEMPLATE_ID": "template_qkfkuzi",
    "VITE_UPDATE_NOTIFICATION_TEMPLATE_ID": "template_5uv23ip"
  }
} 